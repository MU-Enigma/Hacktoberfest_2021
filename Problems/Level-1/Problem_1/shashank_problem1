#include<iostream>
#include<ctype.h>  
using namespace std; 

int main()
{
    int t;
    cin >> t; 
    while(t--)
    {
        string s;
        cin >> s;
        for(int i=0; i<s.length(); ++i) 
        {
            if(islower(s[i]))
                s[i] = toupper(s[i]);
            else 
                s[i] = tolower(s[i]); 
        }
        reverse(s.begin(),s.end());
        cout << s << "\n";
    }
    return 0;
}
